<h2 class="kingSideBarTitle">Group Messages</h2>
<% if(currentUser) { %> 
    <% for(group of currentUser.groupchats) { %>
        <a class="d-flex flex-row" href="/groupchat/<%= group._id  %>">
            <img class="me-2" src="https://images.unsplash.com/photo-1603415526960-f7e0328c63b1?ixid=MnwxMjA3fDB8MHxzZWFyY2h8NHx8cHJvZmlsZSUyMHBpY3R1cmV8ZW58MHx8MHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="img">
            <%= group.name %> 
        </a>
    <% } %> 
<% } %> 
